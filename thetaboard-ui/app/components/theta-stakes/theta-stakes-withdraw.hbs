<div class="card-body-stake">
    <h5 class="title">Withdraw Stake</h5>
    <br/>
    <form class="stakes-withdraw">
    <div class="form-group">
        <label for="stake-amount">You are about to withdraw your Theta.</label>
    </div>
    </form>
    <br/>
    <button {{on "click" this.thetaStakes.showStakes}} type="submit" class="btn btn-fill btn-secondary btn-left">Cancel</button>
    <button {{on "click" this.thetaStakes.withdraw}} type="submit" class="btn btn-fill btn-primary">Review Withdraw</button>
</div>